{
    "DatastoreName": "mydatastore1",
    "Tags": [
        {
            "Key": "key1",
            "Value": "value1"
        },
        {
            "Key": "key2",
            "Value": "value2"
        }
    ],
    "DatastoreStorage": {
        "CustomerManagedS3": {
            "Bucket": "cfn-testing-bucket",
            "RoleArn": "arn:aws:iam::<account_id>:role/cfn-testing-role",
            "KeyPrefix": "iota-prefix/"
        }
    },
    "FileFormatConfiguration": {
        "ParquetConfiguration": {
            "SchemaDefinition": {
                "Columns": [
                    {
                        "Name": "id",
                        "Type": "bigint"
                    },
                    {
                        "Name": "temp",
                        "Type": "double"
                    }
                ]
            }
        }
    }
}
