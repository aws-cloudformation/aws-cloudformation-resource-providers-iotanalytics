{
    "DatastoreName": "mydatastore1",
    "Tags": [
        {
            "Key": "key2",
            "Value": "value22"
        },
        {
            "Key": "key3",
            "Value": "value3"
        },
        {
            "Key": "key4",
            "Value": "value4"
        }
    ],
    "DatastoreStorage": {
        "CustomerManagedS3": {
            "Bucket": "cfn-testing-bucket",
            "RoleArn": "arn:aws:iam::<account_id>:role/cfn-testing-role",
            "KeyPrefix": "iota-prefix/"
        }
    },
    "FileFormatConfiguration": {
        "ParquetConfiguration": {
            "SchemaDefinition": {
                "Columns": [
                    {
                        "Name": "id",
                        "Type": "bigint"
                    },
                    {
                        "Name": "temp",
                        "Type": "double"
                    },
                    {
                        "Name": "velocity",
                        "Type": "float"
                    }
                ]
            }
        }
    },
    "DatastorePartitions": {
        "Partitions": [
            {
                "Partition": {
                    "AttributeName": "attributeFoo"
                }
            },
            {
                "TimestampPartition": {
                    "AttributeName": "timeAttribute",
                    "TimestampFormat": "yyyy-MM-dd HH:mm:ss"
                }
            },
            {
                "Partition": {
                    "AttributeName": "attributeBar"
                }
            }
        ]
    }
}
