{
  "DatasetName": "mydataset2",
  "Tags": [
    {
      "Key": "key1",
      "Value": "value1"
    },
    {
      "Key": "key2",
      "Value": "value2"
    }
  ],
  "ContentDeliveryRules": [
    {
      "EntryName": "custom/fileName.csv",
      "Destination": {
        "S3DestinationConfiguration": {
          "Bucket": "cfn-testing-bucket",
          "RoleArn": "arn:aws:iam::<account_id>:role/cfn-testing-role",
          "Key": "iota-dataset"
        }
      }
    }
  ],
  "Triggers": [
    {
      "Schedule": {
        "ScheduleExpression": "cron(0/5 * * * ? *)"
      }
    }
  ],
  "VersioningConfiguration": {
    "MaxVersions": 10,
    "Unlimited": false
  },
  "Actions": [
    {
      "ActionName": "container_action",
      "ContainerAction": {
        "Image": "<account_id>.dkr.ecr.us-east-1.amazonaws.com/cfn-testing-container",
        "ExecutionRoleArn": "arn:aws:iam::<account_id>:role/cfn-testing-role",
        "ResourceConfiguration": {
          "VolumeSizeInGB": 1,
          "ComputeType": "ACU_2"
        },
        "Variables": [
          {
            "VariableName": "var1",
            "OutputFileUriValue": {
              "FileName": "fileName.csv"
            }
          },
          {
            "VariableName": "var2",
            "DoubleValue": 9.9
          },
          {
            "VariableName": "var3",
            "DatasetContentVersionValue": {
              "DatasetName": "datasetName"
            }
          },
          {
            "VariableName": "var4",
            "StringValue": "str"
          }
        ]
      }
    }
  ]
}
